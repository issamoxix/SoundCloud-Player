\r\n      p.play();\r\n    } else {\r\n      p.pause();\r\n    }\r\n  };\r\n  useEffect(() => {\r\n    SongId && setTrack(SongId);\r\n    setAuto(Playsong);\r\n    if (track_id !== 0) {\r\n      if (Playsong) {\r\n        StartPlaying(1);\r\n      } else {\r\n        StartPlaying(0);\r\n      }\r\n    }\r\n  }, [SongId, Playsong]);\r\n  return (\r\n    <div className=\"SongPlayer\">\r\n      {track_id === 0 ? (\r\n        <h2 style={{ color: \"gray\", fontWeight: 500 }}>Select a song</h2>\r\n      ) : (\r\n        <>\r\n          <iframe\r\n            id=\"myFrame\"\r\n            title=\"player\"\r\n            width=\"100%\"\r\n            height=\"366\"\r\n            scrolling=\"no\"\r\n            frameborder=\"no\"\r\n            allow=\"autoplay\"\r\n            src={src}\r\n          ></iframe>\r\n\r\n          <button\r\n            className=\"PlayButton\"\r\n            onClick={() => {\r\n              var p = Go();\r\n              {\r\n                autop ? p.play() : p.pause();\r\n              }\r\n\r\n              db.collection(\"users\")\r\n                .doc(user.email)\r\n                .update({ playing: !autop });\r\n            }}\r\n          >\r\n            {autop ? \"Pause\" : \"Play\"}{\" \"}\r\n          </button>\r\n          <div className=\"PlayerInfo\">\r\n            <h3>\r\n              Track Id <span> {track_id}</span>{\" \"}\r\n            </h3>\r\n          </div>\r\n        </>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default SongPlayer;\r\n","C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\Search.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\SongRe.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\features\\player\\playerSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\features\\appSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\Login.js",[