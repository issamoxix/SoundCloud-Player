    <Navbar />\r\n          <SongPlayer />\r\n          <Search />\r\n        </>\r\n      ) : (\r\n        <Login />\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\app\\store.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\SongPlayer.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\Search.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\SongRe.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\features\\player\\playerSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\features\\appSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\Login.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\utils\\firebase.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\features\\user\\userSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\soundcloud-player\\src\\components\\Navbar.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":66,"column":6,"nodeType":"61","endLine":66,"endColumn":23,"suggestions":"62"},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'user'. Either include it or remove the dependency array.","ArrayExpression",["65"],"no-global-assign","no-unsafe-negation",{"desc":"66","fix":"67"},"Update the dependencies array to be: [dispatch, check, user]",{"range":"68","text":"69"},[1984,2001],"[dispatch, check, user]"]